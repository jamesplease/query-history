{
  "name": "query-history",
  "version": "2.0.0-beta.0",
  "description": "A history with improved query parameter support.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist es tmp lib",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run clean && npm run build:types && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesplease/query-history.git"
  },
  "keywords": [
    "history",
    "router",
    "routing",
    "query",
    "parameters",
    "query-string",
    "qs"
  ],
  "author": "James Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesplease/query-history/issues"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "types/index.d.ts"
  ],
  "homepage": "https://github.com/jamesplease/query-history#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@types/history": "^4.7.5",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.11",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^24.9.0",
    "rimraf": "^2.6.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "history": "^4.10.1",
    "query-string": "^6.11.0"
  }
}
